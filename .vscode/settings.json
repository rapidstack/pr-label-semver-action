{
  "cSpell.words": [
    "autobuild",
    "codeql",
    "esbuild",
    "nodenext",
    "outdir",
    "prereleases",
    "rapidstack",
    "thollander",
    "typecheck",
    "vitest",
    "Zack",
    "zackheil"
  ],
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json][typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  }
}
